{
  "name": "oscnews",
  "version": "1.0.0",
  "description": "开源中国新闻",
  "icons": {
    "128": "osc-logo.png"
  },
  "permissions": [
    "*://*/*",
    "declarativeContent",
    "tabs",
    "unlimitedStorage",
    "history",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "osc-logo.png"
  },
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}